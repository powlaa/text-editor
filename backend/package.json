{
    "name": "tiptap-sockets",
    "version": "1.0.0",
    "description": "A simple websocket server for collaboration.",
    "main": "server.js",
    "scripts": {
        "start": "nodemon -r esm ./node_modules/@babel/node/bin/babel-node.js server.js"
    },
    "dependencies": {
        "@babel/core": "^7.2.0",
        "@babel/node": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/preset-env": "^7.2.0",
        "@babel/runtime": "^7.2.0",
        "cors": "^2.8.5",
        "esm": "^3.2.25",
        "express": "^4.16.4",
        "prosemirror-model": "^1.7.0",
        "prosemirror-transform": "^1.1.3",
        "socket.io": "2.2.0"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "useBuiltIns": "entry"
                }
            ]
        ],
        "comments": false,
        "compact": true
    },
    "engines": {
        "node": "11.x"
    },
    "repository": {
        "url": "https://glitch.com/edit/#!/tiptap-sockets"
    },
    "license": "MIT",
    "keywords": [
        "node",
        "glitch",
        "express"
    ],
    "devDependencies": {
        "nodemon": "^2.0.6"
    }
}
